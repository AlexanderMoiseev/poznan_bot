import datetime
import telegram
import logging
from telegram.ext import Updater
from telegram.ext import CommandHandler

updater = Updater(token='227177078:AAFNSuNhLu87Y5GEOmXpPAetO8zpv-XLEw8')

dispatcher = updater.dispatcher

logging.basicConfig(format='%(asctime)s - %(name)s - %(levelname)s - %(message)s', level=logging.INFO)

def start(bot, update):
    bot.sendMessage(chat_id = update.message.chat_id, text = "test: /cos")

start_handler = CommandHandler('start', start)
dispatcher.add_handler(start_handler)

updater.start_polling()

def cos (bot, update, args):

    bot.sendMessage(chat_id = update.message.chat_id, text = "test"

kino_handler = CommandHandler('cos', cos, pass_args=True)
dispatcher.add_handler(cos_handler)
